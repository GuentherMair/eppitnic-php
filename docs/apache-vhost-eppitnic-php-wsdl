NameVirtualHost *:8090
Listen 8090

<VirtualHost *:8090>
  DocumentRoot /var/www/eppitnic/public/
  LogLevel notice
  ErrorLog /var/log/apache2/wsdl.error.log
  CustomLog /var/log/apache2/wsdl.access.log combined

  DirectoryIndex wsdl.php

  # E_ERROR | E_USER_WARNING = 1 | 512 = 513
  php_admin_value error_reporting 513

  AddDefaultCharset UTF-8
</VirtualHost>
